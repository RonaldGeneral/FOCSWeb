  const rowSubject = document.createElement('label');
  rowSubject.className = 'col-sm-6 control-label text-right';
  rowSubject.style.fontWeight = 'bold';
  rowSubject.textContent = 'Subject';
  const rowGrade = document.createElement('label');
  rowGrade.className = 'col-sm-6 control-label text-right';
  rowGrade.style.fontWeight = 'bold';
  rowGrade.textContent = 'Grade';
  highqualDiv.appendChild(rowSubject);
  highqualDiv.appendChild(rowGrade);

  const createSubjectGradeRow = (subjects, grades) => {
    const rowDiv = document.createElement('div');
    rowDiv.className = 'form-group';

    const subjectSelect = document.createElement('select');
    subjectSelect.name = 'fhighsubject';
    subjectSelect.className = 'form-control col-sm-5';

    const defaultSubjectOption = document.createElement('option');
    defaultSubjectOption.value = '';
    defaultSubjectOption.textContent = 'Select Subject';
    subjectSelect.appendChild(defaultSubjectOption);

    subjects.forEach(subject => {
      const option = document.createElement('option');
      option.value = subject;
      option.textContent = subject;
      subjectSelect.appendChild(option);
    });

    const gradeSelect = document.createElement('select');
    gradeSelect.name = 'fhighgrade';
    gradeSelect.className = 'form-control col-sm-5';

    const defaultGradeOption = document.createElement('option');
    defaultGradeOption.value = '';
    defaultGradeOption.textContent = 'Select Grade';
    gradeSelect.appendChild(defaultGradeOption);

    grades.forEach(grade => {
      const option = document.createElement('option');
      option.value = grade;
      option.textContent = grade;
      gradeSelect.appendChild(option);
    });

    const subjectColDiv = document.createElement('div');
    subjectColDiv.className = 'col-sm-6';
    subjectColDiv.appendChild(subjectSelect);

    const gradeColDiv = document.createElement('div');
    gradeColDiv.className = 'col-sm-6';
    gradeColDiv.appendChild(gradeSelect);

    rowDiv.appendChild(subjectColDiv);
    rowDiv.appendChild(gradeColDiv);

    return rowDiv;
  };

  const subjects = qualification === 'ST' ? subjectsST : qualification === 'AL' ? subjectsAL : subjectsUE;
  const grades = qualification === 'ST' ? gradesST : qualification === 'AL' ? gradesAL : gradesUE;
  highqualDiv.appendChild(createSubjectGradeRow(subjects, grades));

  const addButton = document.createElement('button');
  addButton.textContent = 'Add Subject';
  addButton.className = 'btn btn-sm btn-primary';
  addButton.onclick = function(event) {
    event.preventDefault();
    highqualDiv.insertBefore(createSubjectGradeRow(subjects, grades), addButton);
  };

  highqualDiv.appendChild(addButton);
}

function loadTarumtQualification(qualification) {
  const foundationDiv = document.getElementById('foundationDiv');
  const diplomaDiv = document.getElementById('diplomaDiv');

  // Hide both divs initially
  if(!qualification){
	foundationDiv.style.display = 'none';
  	diplomaDiv.style.display = 'none';
  }
  

  // Display the appropriate div based on the selected qualification
  if (qualification === 'F') {
    foundationDiv.style.display = 'block';
	diplomaDiv.style.display = 'none';
  } else if (qualification === 'D') {
    diplomaDiv.style.display = 'block';
	foundationDiv.style.display = 'none';
  }
}

function loadPostQualification(qualification) {

  if (!qualification) {
	document.getElementById('postgraduateDiv').style.display = 'none';
    return;
  }

  if(qualification == 'BA'){
	document.getElementById('postgraduateDiv').style.display = 'block';
  }


}

function collectAndSubmitData(event) {
  event.preventDefault(); // Prevent default form submission

  // Collect SPM/O Level data
  const lowQualData = collectSubjectGradeData('fsubject', 'fgrade');

  // Collect STPM/A Level/UEC data
  const highQualData = collectSubjectGradeData('fhighsubject', 'fhighgrade');

  // Collect TAR UMT data
  let tarumtData = {};
  const tarumtQualification = document.getElementById('fquatarumt').value;
  if (tarumtQualification === 'F') {
    tarumtData = {
      qualification: 'Foundation',
      program: document.getElementById('foundationIn').value
    };
  } else if (tarumtQualification === 'D') {
    tarumtData = {
      qualification: 'Diploma',
      program: document.getElementById('diplomaIn').value,
      cgpa: document.getElementById('cgpa').value
    };
  }

  // Collect Postgraduate data if not 'None'
  let postGraduateData = {};
  const postQualification = document.getElementById('postQualification').value;
  if (postQualification !== '') {
    postGraduateData = {
      hasComputingMathBachelor: document.getElementById('fothqua').checked,
	  studyEnglish: document.getElementById('fstudiesEnglish').checked,
      workExperience: document.getElementById('fworkexp').value,
      latestCGPA: document.getElementById('fcgpa').value
    };
  }

  // Collect English Proficiency data
  const englishQualification = document.getElementById('fenglishQua').value;
  let englishProficiencyData = {};
  if (englishQualification === 'MU') {
    englishProficiencyData = {
      qualification: englishQualification,
      band: document.getElementById('fmuetband').value
    };
  } else if (englishQualification === 'IE') {
    englishProficiencyData = {
      qualification: englishQualification,
      bandScore: document.getElementById('fieltsband').value
    };
  } else if (englishQualification === 'TP') {
    englishProficiencyData = {
      qualification: englishQualification,
      score: document.getElementById('ftoeflscore').value
    };
  } 

  // Combine all data
  const allData = {
    lowQual: lowQualData,
    highQual: highQualData,
    tarumt: tarumtData,
    postGraduate: postGraduateData,
    englishProficiency: englishProficiencyData
  };

  // Store data in hidden field
  document.getElementById('subjectGradeData').value = JSON.stringify(allData);
  console.log(JSON.stringify(allData));
  // Submit the form
  document.form1.submit();
}

function collectSubjectGradeData(subjectName, gradeName) {
  const data = [];
  const subjects = document.getElementsByName(subjectName);
  const grades = document.getElementsByName(gradeName);

  for (let i = 0; i < subjects.length; i++) {
    if (subjects[i].value && grades[i].value) {
      data.push({
        subject: subjects[i].value,
        grade: grades[i].value
      });
    }
  }

  return data;
}
	
