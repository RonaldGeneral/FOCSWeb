	function check_EnglishQua(engqua) {
		if(engqua == "MU") {
			$('#divMUET').show();
			$('#divIELTS').hide();
			$('#divTOEFL').hide();
			$('#divLS').hide();	
		} else if(engqua == "IE") {
			$('#divMUET').hide();
			$('#divIELTS').show();
			$('#divTOEFL').hide();
			$('#divLS').hide();	
		} else if(engqua == "TP" || engqua == "TI" || engqua == "TC") {
			$('#divMUET').hide();
			$('#divIELTS').hide();
			$('#divTOEFL').show();
			$('#divLS').hide();	
		} else if(engqua == "LS") {
			$('#divMUET').hide();
			$('#divIELTS').hide();
			$('#divTOEFL').hide();
			$('#divLS').show();	
		} else {
			$('#divMUET').hide();
			$('#divIELTS').hide();
			$('#divTOEFL').hide();
			$('#divLS').hide();	
		}
	}
	
	
	function  checkDecimal(evt){
		evt = (evt) ? evt : window.event;
		var charCode = (evt.which) ? evt.which : evt.keyCode;
		if (charCode > 47 && charCode < 58)
			return true;
		else if(charCode==13){ //Enter
			forms.submit();
			return false;
		}else
			return false;
	}
	
