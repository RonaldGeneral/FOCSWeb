		<style>
            body{
                display: flex;
                flex-direction: column;
                display: grid;
            }
            .QZ3zWd{
                min-height:2400px;
                max-height:10000px;
                display: flex;
                flex-direction: column;
            }
		</style>
	</head>

	<body class="no-skin">
        <%- include('layouts/partials/header', { isStatic: false }) %>
        <style>
            .Pvc6xe {
                position: fixed;
                z-index: 1030;
                background-color: orange;
            }
            .Pvc6xe a,svg{
                color: black !important;
            }
            body {
                padding-top: 50px;
            }
        </style>
        <div class="main-content">
            <div class="main-content-inner">
                <div class="page-content">
                    <div class="row">
                        <div class="col-xs-12">
                            <h1>Available Programmes</h1>
        
                            <% if (programmes.length > 0) { %>
                            <ul>
                                <% programmes.forEach(function(programme) { %>
                                <li>
                                    <strong><%= programme.ProgrammeName %></strong> - <%= programme.StudyYear %> year(s)
                                    <ul>
                                    <li>Minimum Requirements:
                                        <ul>
                                        <% programme.requirements.forEach(function(req) { %>
                                            <li>
                                            <%= req.criteria %>: <%= req.detail %>
                                            </li>
                                        <% }); %>
                                        </ul>
                                    </li>
                                    </ul>
                                </li>
                                <% }); %>
                            </ul>
                            <% } else { %>
                            <p>No programmes match your qualifications. Please review your input or contact an advisor for more information.</p>
                            <% } %>
        
                        </div>
                    </div>
                </div>

            </div>

        </div>
        

    

    </body>
