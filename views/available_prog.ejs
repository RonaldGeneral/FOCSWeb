		<!--[if lte IE 9]>
		  <link rel="stylesheet" href="https://web.tarc.edu.my/portal/assets/css/ace-ie.css" />
		<![endif]-->

		<!-- inline styles related to this page -->

		<!-- ace settings handler -->
		<script src="https://web.tarc.edu.my/portal/assets/js/ace-extra.js"></script>

		<!-- HTML5shiv and Respond.js for IE8 to support HTML5 elements and media queries -->

		<!--[if lte IE 8]>
		<script src="https://web.tarc.edu.my/portal/assets/js/html5shiv.js"></script>
